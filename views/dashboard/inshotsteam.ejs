<% include includes/header.ejs %>

        <h2>Insert the Data for Shots Acuracy:</h2>
        <% if (err !== null) { %>
            <div class="alert alert-danger alert-dismissible fade in">
                <a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
                <strong><%= err %></strong>
            </div>
            <% } %>
            <% if (msg !== null) { %>
            <div class="alert alert-info alert-dismissible fade in">
                <a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
                <strong><%= msg %></strong>
            </div>
            <% } %>
        <form action="/dashboard/inshotsteam" method="POST" id="post-item-form">
            <div class="row">
                <div class="col-md-6">
                    
                    <div class="form-group">
                        <label for="Club_id">Club Id:</label>
                        <input type="text" class="form-control" name="Club_id" required>
                    </div>
                    <div class="form-group">
                        <label for="Shots_on_target">Shots made:</label>
                        <input type="text" class="form-control" name="Shots_on_target" required >
                    </div>
                    <div class="form-group">
                        <label for="Shot_accuracy">Shots accuracy:</label>
                        <input type="text" class="form-control" name="Shot_accuracy" required>
                    </div>
                    
              
                    
                    <button class="btn btn-primary btn-block">
                        Add Details
                    </button>
                </div>
            </div>
        </form>

<% include includes/footer.ejs %>